{
  "sketch": {
    "extraction": {
      "keywords": ["Button", "TextView", "TextInput", "Image"],
      "exceptions": [
        "iPhone X Frame",
        "Color",
        "Overrides",
        "Tint",
        "Mask",
        "Pin",
        "Guide",
        "Background",
        "Tab Bar",
        "Navigation Bar",
        "Keyboard"
      ],
      "followOverrides": true,
      "symbols": {
        "Button": {
          "Label": "text",
          "Background": "rectangle",
          "Icon": "symbolInstance"
        },
        "TextView": {
          "Description": "text",
          "Background": "rectangle"
        },
        "TextInput": {
          "Placeholder": "text",
          "Background": "rectangle"
        },
        "Image": {
          "Image": "rectangle",
          "Background": "rectangle"
        },
        "Card": {
          "Title": "text",
          "Description": "text",
          "Image": "rectangle",
          "Background": "rectangle"
        }
      }
    },
    "namingRule": {
      "page": {
        "pattern": "%s",
        "upperCamelCase": false,
        "englishOnly": true,
        "unique": true,
        "maxLength": 20
      },
      "artboard": {
        "pattern": "%s / %s",
        "lowerCamelCase": true,
        "englishOnly": true,
        "unique": true,
        "maxLength": 50
      }
    }
  }
}
