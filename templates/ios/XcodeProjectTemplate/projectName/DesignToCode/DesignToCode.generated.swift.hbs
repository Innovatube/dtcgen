import Foundation
import UIKit

struct Dtc {
    struct config {
        
        static var baseViewComponentName = "BaseView"

        {{#forEach names}}
        static var {{name}}: ViewConfig {
            get {
                return {{name}}Config()
            }
        }
        {{/forEach}}
    }

    struct viewId {
        {{#forEach tree}}
        struct {{name}} {
            static let value = "{{uid}}"
        {{> structViewId nodes=elements}}
        }
        {{/forEach}}
    }
}
