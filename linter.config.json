{
  "sketch": {
    "targetSketchFilePath": "/Users/mito/Downloads/linterSample.sketch",
    "outFilePath": "/Users/mito/Downloads/layers.json",
    "extraction": {
      "keywords": ["Button"],
      "maxHierarchy": 3,
      "followOverrides": true,
      "button": {
        "label": "text",
        "background": "rectangle"
      }
    },
    "namingRule": {
      "page": {
        "pattern": "%s",
        "upperCamelCase": false,
        "englishOnly": true,
        "unique": true,
        "maxLength": 20
      },
      "artboard": {
        "pattern": "%s / %s",
        "lowerCamelCase": true,
        "englishOnly": true,
        "unique": true,
        "maxLength": 50
      }
    }
  }
}
