{
  "sketch": {
    "extraction": {
      "keywords": ["Button", "TextView"],
      "maxHierarchy": 3,
      "followOverrides": true,
      "button": {
        "label": "text",
        "background": "rectangle",
        "icon": "symbolInstance"
      }
    },
    "namingRule": {
      "page": {
        "pattern": "%s",
        "upperCamelCase": false,
        "englishOnly": true,
        "unique": true,
        "maxLength": 20
      },
      "artboard": {
        "pattern": "%s / %s",
        "lowerCamelCase": true,
        "englishOnly": true,
        "unique": true,
        "maxLength": 50
      }
    }
  }
}
